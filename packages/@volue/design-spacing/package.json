{
  "name": "@volue/design-spacing",
  "version": "0.0.0",
  "description": "Spacing primitives for Volue design primitives",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/PowelAS/design-primitives.git",
    "directory": "packages/@volue/design-spacing"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.common.js",
  "module": "dist/index.module.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --cache --fix --ext \".js,.ts\" --ignore-path ../../../.prettierignore",
    "clean": "rimraf dist coverage",
    "_theo": "theo --transform web --dest dist",
    "prebuild": "npm run clean",
    "build:tokens": "npm run _theo -s -- --format scss,json,common.js,module.js,custom-properties.css,cssmodules.css,html src/index.yml",
    "build:tokensMap": "npm run _theo -s -- --format map.scss src/spacing.yml",
    "build": "run-p build:*"
  },
  "devDependencies": {
    "cpy-cli": "3.0.0",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "theo": "8.1.5",
    "typescript": "3.7.4"
  }
}
